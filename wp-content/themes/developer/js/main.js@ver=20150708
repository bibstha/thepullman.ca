function initScrollReveal() {
	'use strict';
	var el = jQuery('.timeline ul li');
	var config = {
	        vFactor: 0.20,
		complete:function( el ) {
			jQuery(el).addClass('pg-developer-point');
	}
	      }
		 
		   if(window.pageYOffset<2) {
window.sr = new scrollReveal(config);
}
}

