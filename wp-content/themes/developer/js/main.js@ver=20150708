/* Forms
/*--------------------------*/
	   jQuery('input').on('blur', function(){
		   'use strict';
	      jQuery(this).parent().next('label').removeClass('input-pg-developer-hover').addClass('input-dv');
	   }).on('focus', function(){
		   'use strict';
	     jQuery(this).parent().next('label').removeClass('input-dv').addClass('input-pg-developer-hover');
	   });
	  
